#include<iostream>
using namespace std;

int main()
{
	int i, j, k, rows=6;
   
    cout << "Pyramid  Pattern\n"; 

    for(i = 1; i <= rows; i++)
    {
    	for(j = 0; j <= rows - i; j++)
		{
            cout << " ";
        }
        for(k = 0; k < i * 2 - 1; k++)
        {
            cout << "#";
        }
        cout << "\n";
    }		
 	return 0;
}
